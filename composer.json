{
    "name": "php-etl/akeneo-plugin",
    "description": "Adapters for the Akeneo API client",
    "type": "gyroscops-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "nikic/php-parser": "^4.13.2",
        "symfony/config": "^5.2",
        "symfony/expression-language": "^5.2",
        "php-etl/configurator-contracts": "^0.5.0",
        "php-etl/fast-map-plugin": "^0.7.0",
        "php-etl/packaging-contracts": "^0.1.0",
        "php-etl/satellite-toolbox": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpunit/php-invoker": "*",
        "johnkary/phpunit-speedtrap": "*",
        "php-http/mock-client": "^1.4@dev",
        "symfony/yaml": "^5.2",
        "laminas/laminas-diactoros": "^2.6",
        "mybuilder/phpunit-accelerator": "*",
        "php-etl/pipeline-contracts": "^0.3.0",
        "php-etl/bucket-contracts": "^0.1.0",
        "php-etl/bucket": "^0.2.0",
        "guzzlehttp/psr7": "^2.0",
        "php-http/message": "^1.11",
        "php-http/curl-client": "^2.0",
        "mikey179/vfsstream": "^1.6",
        "infection/infection": "^0.26.10",
        "php-etl/phpunit-extension": "^0.4",
        "fakerphp/faker": "^1.19",
        "akeneo/api-php-client": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Plugin\\Akeneo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Plugin\\Akeneo\\": "tests/functional/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.5.x-dev"
        },
        "gyroscops": {
            "plugins": ["Kiboko\\Plugin\\Akeneo\\Service"]
        }
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
