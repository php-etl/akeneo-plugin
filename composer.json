{
    "name": "php-etl/akeneo-plugin",
    "description": "Adapters for the Akeneo API client",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\ETL\\Flow\\Akeneo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Component\\ETL\\Flow\\Akeneo\\": "tests/functional/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": "8.0",
        "ext-json": "*",
        "php-etl/configurator-contracts": "dev-master",
        "nikic/php-parser": "^4.9@dev",
        "symfony/config": "^4.0 || ^5.0"
    },
    "require-dev": {
        "php-http/mock-client": "^1.4@dev",
        "symfony/yaml": "5.x-dev",
        "symfony/console": "5.x-dev",
        "php-etl/bucket-contracts": "^0.1.0@dev",
        "akeneo/api-php-client-ee": "^4.0",
        "php-etl/pipeline-contracts": "^0.1.0@dev",
        "php-etl/pipeline": "^0.2.0@dev",
        "phpunit/phpunit": "^9.0",
        "laminas/laminas-diactoros": "2.6.x-dev",
        "adlawson/vfs": "dev-develop"
    },
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
